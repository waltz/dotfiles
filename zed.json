{
  "ui_font_size": 14,
  "buffer_font_size": 14,
  "show_whitespaces": "all",
  "remove_trailing_whitespace_on_save": true,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "theme": {
    "mode": "system",
    "light": "0x96f Theme",
    "dark": "0x96f Theme"
  },
  "auto_install_extensions": {
    "html": true,
    "catpuccin": true,
    "ruby": true,
    "Quill Theme": true,
    "0x96f": true
  }
}
